<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <template id="website.assets_frontend" name="Website Frontend Assets" inherit_id="website.assets_frontend">
            <xpath expr="//link[last()]" position="after">
                <link rel="stylesheet" type="text/css" href="/my_module/static/css/style.css"/>
            </xpath>
            <xpath expr="//script[last()]" position="after">
                <script type="text/javascript" src="/my_module/static/js/script.js"/>
            </xpath>
        </template>
    </data>
</odoo>
